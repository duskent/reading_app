{
  "name": "reading_app",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Julian Ronayi <yuliy.ronayi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node build/server.js",
    "server": "nodemon build/server.js",
    "server-watch": "BABEL_ENV=production NODE_ENV=production webpack --config ./config/server.config.js -w",
    "client": "node scripts/start.js",
    "prebuild": "rimraf build",
    "build": "node scripts/build.js",
    "postbuild": "BABEL_ENV=production NODE_ENV=production webpack --config ./config/server.config.js",
    "test": "jest --forceExit --coverage && codecov -t cfe7f70c-b7b6-40e6-a44d-25ab3f033979",
    "test-last-commit": "jest --forceExit --no-cache --onlyChanged --lastCommit",
    "test:watch": "jest --watch",
    "lint": "eslint --cache ./server",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test-storybook": "storyshots"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/public",
      "/build"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/server/db/**",
      "**/server/graphql/**",
      "**/client/**",
      "!**/node_modules/**",
      "!**/server/db/index.js",
      "!**/server/graphql/index.js",
      "!**/server/test/**"
    ],
    "moduleNameMapper": {
      "^mongoose$": "<rootDir>/node_modules/mongoose"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "precommit": "lint",
  "dependencies": {
    "autoprefixer": "7.1.4",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-preset-react-app": "3.0.2",
    "babel-runtime": "6.26.0",
    "bootstrap": "4.0.0-alpha.6",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.1.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.7.2",
    "eslint-config-react-app": "2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.3.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "4.0.2",
    "graphql": "^0.10.3",
    "html-webpack-plugin": "2.30.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.10.8",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "pre-push": "^0.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-transition-group": "^15.6.2",
    "react-dev-utils": "4.0.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^4.8.0",
    "rimraf": "^2.6.2",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "transliteration": "^1.6.2",
    "url-loader": "0.5.9",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storyshots": "^2.1.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "codecov": "^2.3.0",
    "compression-webpack-plugin": "^1.0.0",
    "eslint": "^4.6.1",
    "eslint-plugin-react": "^7.3.0",
    "jest": "21.0.2",
    "jest-cli": "21.0.2",
    "nodemon": "^1.12.0",
    "pre-commit": "^1.2.2",
    "regenerator-runtime": "^0.11.0",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.7.1"
  }
}
